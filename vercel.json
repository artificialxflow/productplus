{
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "true",
    "NEXTAUTH_URL": "https://swpl.ir",
    "NEXT_PUBLIC_DOMAIN": "https://swpl.ir"
  }
}
